{
    "name": "svelte-repo",
    "description": "svelte-repo",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "svelte-eslint": "eslint --ext .ts,.svelte ./App",
        "svelte-build": "npm run svelte-scss-build && npm run svelte-build-all",
        "svelte-run": "concurrently --names \"TS,SCSS\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:svelte-watch-all\" \"npm:svelte-scss-watch\" ",
        "svelte-build-all": "node ./App/svelte-pipeline/build-all.js",
        "svelte-watch-all": "node ./App/svelte-pipeline/watch-all.js",
        "svelte-scss-build": "sass ./App/scss/style.scss ./dist/build/style.css --no-source-map --style compressed",
        "svelte-scss-watch": "sass ./App/scss/style.scss ./dist/build/style.css -w --style compressed",
        "svelte-test": "jest App",
        "svelte-test:watch": "npm run test -- --watch"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^14.0.27",
        "chai": "^4.2.0",
        "fs.realpath": "^1.0.0",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-concat": "^2.6.1",
        "gulp-css-base64": "^1.3.4",
        "gulp-if": "^2.0.2",
        "gulp-less": "^4.0.1",
        "gulp-livereload": "^4.0.1",
        "gulp-ng-module-sort": "0.0.3",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-typescript": "^5.0.0",
        "gulp-watch": "^5.0.1",
        "indx": "^0.2.3",
        "livereload": "^0.9.1",
        "mocha": "^8.2.1",
        "natives": "^1.1.6",
        "options": "0.0.6",
        "ts-node": "^9.0.0",
        "typescript": "^4.9.5",
        "uglify-js": "^3.4.9",
        "when": "^3.7.8",
        "yargs": "^5.0.0",
        "@rollup/plugin-commonjs": "^25.0.1",
        "@rollup/plugin-json": "6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@tsconfig/svelte": "^3.0.0",
        "@types/sanitize-html": "^2.9.0",
        "@types/vanillajs-datepicker": "^1.2.2",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "concurrently": "^8.2.0",
        "eslint-plugin-svelte": "^2.30.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-junit": "^16.0.0",
        "rollup": "^3.25.1",
        "rollup-plugin-css-only": "^4.3.0",
        "rollup-plugin-svelte": "^7.1.5",
        "sanitize-html": "^2.10.0",
        "sass": "^1.63.4",
        "simple-svelte-autocomplete": "^2.5.2",
        "svelte": "^3.59.1",
        "svelte-check": "^3.4.3",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^5.0.4",
        "svelte-spa-router": "^3.3.0",
        "ts-jest": "^29.1.0",
        "tslib": "^2.5.3",
        "vanillajs-datepicker": "1.3.3",
        "yup": "^1.2.0"
    }
}
